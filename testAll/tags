!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.7	//
TestStruct	.\test.cpp	/^} TestStruct;$/;"	t	typeref:struct:_TestStruct	file:
_TestStruct	.\test.cpp	/^typedef struct _TestStruct {$/;"	s	file:
_TestStruct::i	.\test.cpp	/^    int i;$/;"	m	struct:_TestStruct	file:	access:public
_TestStruct::string	.\test.cpp	/^    char *string;$/;"	m	struct:_TestStruct	file:	access:public
i	.\test.cpp	/^    int i;$/;"	m	struct:_TestStruct	file:	access:public
main	.\test.cpp	/^int main(int argc, char *argv[])$/;"	f	signature:(int argc, char *argv[])
string	.\test.cpp	/^    char *string;$/;"	m	struct:_TestStruct	file:	access:public
